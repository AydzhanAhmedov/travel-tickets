<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<DialogPane fx:id="dialogPane" minHeight="430.0" prefHeight="431.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="bg.tuvarna.traveltickets.controller.ClientDialogController">
    <content>
        <GridPane fx:id="gridPane">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="30.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="70.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="E-mail"/>
                <Label text="Username" GridPane.rowIndex="1"/>
                <Label text="Password" GridPane.rowIndex="2"/>
                <Label text="Client type" GridPane.rowIndex="3"/>
                <Label text="Name" GridPane.rowIndex="4"/>
                <Label text="Phone" GridPane.rowIndex="5"/>
                <Label text="Address" GridPane.rowIndex="6"/>
                <Label text="City" GridPane.rowIndex="7"/>
                <Label text="Region" GridPane.rowIndex="8"/>
                <Label fx:id="detail1Label" text="Detail1" GridPane.rowIndex="9"/>
                <Label fx:id="detail2Label" text="Detail2" GridPane.rowIndex="10"/>
                <TextField fx:id="emailTextField" maxWidth="150.0" prefWidth="150.0" GridPane.columnIndex="1"/>
                <TextField fx:id="usernameTextField" maxWidth="150.0" prefWidth="150.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
                <PasswordField fx:id="passwordField" maxWidth="150.0" prefWidth="150.0" GridPane.columnIndex="1"
                               GridPane.rowIndex="2"/>
                <ComboBox fx:id="clientTypeComboBox" onAction="#onChange" prefWidth="150.0" GridPane.columnIndex="1"
                          GridPane.rowIndex="3"/>
                <TextField fx:id="nameTextField" maxWidth="150.0" prefWidth="150.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="4"/>
                <TextField fx:id="phoneTextField" maxWidth="150.0" prefWidth="150.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="5"/>
                <TextField fx:id="addressTextField" maxWidth="150.0" prefWidth="150.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="6"/>
                <ComboBox fx:id="cityComboBox" editable="true" prefWidth="150.0" GridPane.columnIndex="1"
                          GridPane.rowIndex="7"/>
                <ComboBox fx:id="regionComboBox" editable="true" prefWidth="150.0" GridPane.columnIndex="1"
                          GridPane.rowIndex="8"/>
                <TextField fx:id="detail1TextField" maxWidth="150.0" prefWidth="150.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="9"/>
                <TextField fx:id="detail2TextField" maxWidth="150.0" prefWidth="150.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="10"/>

            </children>
            <padding>
                <Insets left="20.0" right="20.0" top="10.0"/>
            </padding>
        </GridPane>
    </content>
</DialogPane>
